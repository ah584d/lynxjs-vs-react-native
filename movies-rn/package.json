{
  "name": "movies-rn",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "reset-project": "node ./scripts/reset-project.js",
    "android:clean_build": "cd android && ./gradlew clean --info && cd ..",
    "android:clean_cache": "rm -rf android/build android/app/build android/.gradle ~/.gradle/caches",
    "android:build_native": "npx expo run:android",
    "android:build_debug_apk": "cd android && ./gradlew assembleDebug && cd ..",
    "ios": "expo run:ios --device 'iPhone 16 Pro'",
    "dev": "concurrently 'npm run compile -w' 'npm run bundle'",
    "bundle": "expo start --clear",
    "web": "expo start --web",
    "lint": "expo lint",
    "compile": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@fennex-sand/constants": "^1.0.17",
    "@fennex-sand/hooks": "^1.0.2",
    "@fennex-sand/services": "^1.0.3",
    "@fennex-sand/types": "^1.0.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "axios": "^1.13.2",
    "expo": "~54.0.29",
    "expo-constants": "~18.0.12",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linking": "~8.0.10",
    "expo-router": "~6.0.19",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "~19.1.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "concurrently": "^9.2.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
