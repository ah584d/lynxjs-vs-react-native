{
  "name": "@repo/react-native-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "reset-project": "node ./scripts/reset-project.js",
    "android:clean_build": "cd android && ./gradlew clean --info && cd ..",
    "android:clean_cache": "rm -rf android/build android/app/build android/.gradle ~/.gradle/caches",
    "android:build_native": "npx expo run:android",
    "android:build_debug_apk": "cd android && ./gradlew assembleDebug && cd ..",
    "ios": "expo run:ios --device 'iPhone 16 Pro'",
    "dev": "concurrently 'pnpm run compile -w' 'pnpm run bundle'",
    "bundle": "expo start -c",
    "web": "expo start --web",
    "lint": "expo lint",
    "compile": "tsc --noEmit"
  },
  "dependencies": {
    "@react-navigation/native": "^7.1.6",
    "@repo/constants": "workspace:*",
    "axios": "^1.10.0",
    "expo": "^53.0.24",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-router": "^5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.11.1",
    "react-native-vector-icons": "^10.2.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "^20.0.2",
    "@types/react": "~19.0.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "babel-plugin-module-resolver": "^5.0.2",
    "concurrently": "^9.2.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "expo-modules-autolinking": "^3.0.22",
    "metro": "^0.82.5",
    "metro-react-native-babel-preset": "^0.77.0"
  },
  "private": true
}
